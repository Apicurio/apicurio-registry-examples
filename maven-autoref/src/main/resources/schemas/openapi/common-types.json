{
    "openapi": "3.0.2",
    "info": {
        "title": "Common Base Types",
        "version": "1.0.0"
    },
    "components": {
        "schemas": {
            "Person": {
                "title": "Root Type for Person",
                "description": "",
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "height": {
                        "format": "int32",
                        "type": "integer"
                    }
                },
                "example": {
                    "name": "Eric",
                    "height": 134
                }
            },
            "Error": {
                "title": "Root Type for Error",
                "description": "",
                "type": "object",
                "properties": {
                    "code": {
                        "format": "int32",
                        "type": "integer"
                    }
                },
                "example": {
                    "code": 404
                }
            }
        },
        "responses": {
            "NotFound": {
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Error"
                        }
                    }
                },
                "description": ""
            },
            "ServerError": {
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Error"
                        }
                    }
                },
                "description": ""
            }
        }
    }
}